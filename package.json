{
	"name": "docsmith",
	"version": "8.0.0",
	"description": "RESTful API for converting clinical documents and files",
	"main": "src/app.js",
	"repository": "git+https://github.com/Fdawgs/docsmith.git",
	"homepage": "https://github.com/Fdawgs/docsmith",
	"bugs": {
		"url": "https://github.com/Fdawgs/docsmith/issues"
	},
	"license": "MIT",
	"author": "Frazer Smith <frazer.dev@outlook.com>",
	"engines": {
		"node": "^14.17.0 || >=16.0.0"
	},
	"scripts": {
		"benchmark:html": "autocannon -a 100 -m POST -i \"./test_resources/test_files/pdf_1.3_NHS_Constitution.pdf\" -H \"Content-Type:application/pdf\" \"http://0.0.0.0:8204/pdf/html?lastPageToConvert=1\"",
		"benchmark:txt": "autocannon -a 100 -m POST -i \"./test_resources/test_files/pdf_1.3_NHS_Constitution.pdf\" -H \"Content-Type:application/pdf\" \"http://0.0.0.0:8204/pdf/txt?lastPageToConvert=1\"",
		"jest": "jest",
		"jest:coverage": "jest --coverage",
		"lint": "eslint . --cache --ext js,jsx,ts,tsx --ignore-path .gitignore",
		"lint:licenses": "license-checker --direct --excludePackages=\"caniuse-lite@1.0.30001373;duck@0.1.12;language-subtag-registry@0.3.22;tesseract.js-core@2.2.0;url-template@2.0.8\" --onlyAllow=\"0BSD;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;MIT;Python-2.0;Unlicense\" --production --summary",
		"lint:prettier": "prettier . -w -u",
		"lint:prettier:ci": "prettier . -c -u",
		"prepare": "husky install",
		"start": "node .",
		"start:dev": "nodemon | pino-pretty",
		"test": "npm run lint && npm run jest"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"jest": {
		"clearMocks": true,
		"collectCoverageFrom": [
			"src/**/*.js",
			"!src/app.js"
		],
		"coverageReporters": [
			"text",
			"lcovonly"
		],
		"resetMocks": true,
		"testEnvironment": "node",
		"testTimeout": 60000
	},
	"nodemonConfig": {
		"env": {
			"NODE_ENV": "development",
			"SUPPRESS_SUPPORT": 1
		},
		"ignore": [
			"src/temp",
			"test_resources"
		],
		"verbose": true,
		"watch": [
			".env",
			"src"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@faker-js/faker": "^7.4.0",
		"autocannon": "^7.8.1",
		"eslint": "^8.22.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.8.3",
		"eslint-plugin-jsdoc": "^39.3.6",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-security": "^1.5.0",
		"eslint-plugin-security-node": "^1.1.1",
		"husky": "^8.0.1",
		"is-html": "^2.0.0",
		"jest": "^28.1.3",
		"license-checker": "^25.0.1",
		"nodemon": "^2.0.19",
		"pino-pretty": "^8.1.0",
		"playwright": "^1.25.0",
		"prettier": "^2.7.1"
	},
	"dependencies": {
		"@fastify/accepts": "^4.0.0",
		"@fastify/autoload": "^5.2.0",
		"@fastify/bearer-auth": "^8.0.1",
		"@fastify/compress": "^6.1.0",
		"@fastify/cors": "^8.1.0",
		"@fastify/helmet": "^9.1.0",
		"@fastify/rate-limit": "^7.3.0",
		"@fastify/sensible": "^5.1.0",
		"@fastify/static": "^6.5.0",
		"@fastify/swagger": "^7.4.1",
		"@fastify/under-pressure": "^7.0.0",
		"cssesc": "^3.0.0",
		"cssom": "^0.5.0",
		"dotenv": "^16.0.1",
		"env-schema": "^5.0.0",
		"fastify": "^4.5.0",
		"fastify-disablecache": "^3.0.2",
		"fastify-floc-off": "^2.0.2",
		"fastify-plugin": "^4.0.0",
		"file-stream-rotator": "^0.6.1",
		"file-type": "^16.5.4",
		"fix-utf8": "^1.1.1",
		"fluent-json-schema": "^3.1.0",
		"glob": "^8.0.3",
		"htmltidy2": "^1.0.0",
		"jsdom": "^20.0.0",
		"language-tags": "^1.0.5",
		"mammoth": "^1.4.21",
		"node-poppler": "^6.0.0",
		"node-unrtf": "^3.0.0",
		"physical-cpu-count": "^2.0.0",
		"pino": "^8.4.1",
		"raw-body": "^2.5.1",
		"redoc": "^2.0.0-rc.74",
		"secure-json-parse": "^2.5.0",
		"tesseract.js": "^2.1.5",
		"upath": "^2.0.1"
	}
}

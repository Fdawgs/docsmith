{
  "name": "docsmith",
  "version": "2.1.0",
  "description": "RESTful API for converting clinical documents/files",
  "main": "src/app.js",
  "repository": "git+https://github.com/Fdawgs/docsmith.git",
  "homepage": "https://github.com/Fdawgs/docsmith",
  "bugs": {
    "url": "https://github.com/Fdawgs/docsmith/issues"
  },
  "license": "MIT",
  "author": "Frazer Smith <frazer.dev@outlook.com>",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "benchmark": "autocannon -H \"Authorization:Bearer testtoken\" \"http://0.0.0.0:8204/healthcheck\"",
    "jest": "jest --detectOpenHandles --runInBand",
    "jest:coverage": "jest --coverage --runInBand",
    "lint": "eslint . --cache --ext js,jsx,ts,tsx --ignore-path .gitignore",
    "lint:licenses": "license-checker --direct --excludePackages=\"tesseract.js-core@2.2.0\" --onlyAllow=\"0BSD;Apache-2.0;BSD;BSD-2-Clause;BSD-3-Clause;ISC;MIT;Unlicense\" --production --summary",
    "lint:prettier": "prettier . --write --ignore-unknown",
    "lint:prettier:ci": "prettier . --check --ignore-unknown",
    "prepare": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "start": "node .",
    "start:dev": "nodemon",
    "test": "npm run lint && npm run jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ],
    "coverageReporters": [
      "text",
      "lcovonly"
    ],
    "resetMocks": true,
    "testEnvironment": "node",
    "testTimeout": 30000
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development",
      "SUPPRESS_SUPPORT": 1
    },
    "ignore": [
      "src/temp",
      "test_resources"
    ],
    "verbose": true,
    "watch": [
      ".env",
      "src"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "autocannon": "^7.3.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsdoc": "^35.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.5.3",
    "glob": "^7.1.7",
    "husky": "^6.0.0",
    "is-html": "^2.0.0",
    "jest": "^27.0.0",
    "license-checker": "^25.0.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "pino-pretty": "^5.0.0",
    "prettier": "2.3.0"
  },
  "dependencies": {
    "auto-parse": "^1.8.0",
    "cssom": "^0.4.4",
    "dotenv": "^10.0.0",
    "env-schema": "^3.0.1",
    "fastify": "^3.15.1",
    "fastify-autoload": "^3.7.1",
    "fastify-bearer-auth": "^5.1.0",
    "fastify-cors": "^6.0.1",
    "fastify-disablecache": "^2.0.0",
    "fastify-floc-off": "^1.0.1",
    "fastify-helmet": "^5.3.1",
    "fastify-plugin": "^3.0.0",
    "fastify-rate-limit": "^5.5.0",
    "fastify-swagger": "^4.7.0",
    "file-stream-rotator": "^0.5.7",
    "file-type": "^16.4.0",
    "fix-utf8": "^1.1.1",
    "fluent-json-schema": "^3.0.0",
    "glob": "^7.1.7",
    "htmltidy2": "^0.3.0",
    "http-errors": "^1.8.0",
    "jsdom": "^16.6.0",
    "node-poppler": "^4.0.0",
    "node-unrtf": "^2.0.0",
    "pino": "^6.11.3",
    "raw-body": "^2.4.1",
    "tesseract.js": "^2.1.4",
    "under-pressure": "^5.6.0",
    "uuid": "^8.3.2"
  }
}
